{
  "name": "resume",
  "version": "3.2.1",
  "type": "module",
  "homepage": "https://najwer23.github.io/resume/",
  "scripts": {
    "deploy": "yarn run build && gh-pages -d build && rimraf build",
    "start": "vite --open --host",
    "build": "tsc && vite build",
    "preview": "yarn build && vite preview --open --host",
    "god": "ncu -u && yarn install && yarn run start",
    "prepare": "husky",
    "check:prettier": "prettier -c '**/*{json,ts,tsx,js,md,css}'",
    "check:lint": "eslint . && stylelint '**/*css'",
    "check:all": "yarn check:prettier && yarn check:lint",
    "fix:prettier": "prettier -c '**/*{json,ts,tsx,js,md,css}' -w",
    "fix:all": "eslint . --fix && stylelint '**/*.css' --fix && yarn fix:prettier"
  },
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/mulish": "^5.0.18",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "najwer23storybook": "^4.3.17",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^6.24.1",
    "rimraf": "^5.0.8",
    "stylelint": "^16.6.1",
    "stylelint-config-css-modules": "^4.4.0",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  },
  "packageManager": "yarn@4.3.1"
}
